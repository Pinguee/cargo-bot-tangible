body {
  font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
  font-size: 10px;
  font-weight: bold;
  color: #58595b;
}

.cn-game-ui {
  width: 750px;
  text-align: center;
  background-color: #eeeeee;
  padding: 25px;
  margin: auto;
}

div:after {
  content: ' '; /* Makes the div 'fill' the floating sub-divs. */
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
